    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}/dist/server.js",
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ],
            "preLaunchTask": "tsc: build - tsconfig.json",
            "env": {
                "MYSQL_DATABASE": "first-app-nodejs",
                "MYSQL_USERNAME": "root",
                "MYSQL_PASSWORD": null,
                "MYSQL_HOST": "127.0.0.1",
                "MYSQL_PORT": "3306",
                "NODE_ENV": "local",
                "SESSION_SECRET": "",
                "HASH_SECRET": "",
                "S3_PUBLIC_URL": "",
                "API_ENDPOINT": "",
                "PORT": "3000",
                "AWS_IAM_ID": "",
                "AWS_REGION": "",
                "S3_CSV_BUCKET": "",
                "AWS_SQS_IAM_ACCESS_KEY": "",
                "S3_REPORT_BUCKET": "",
                "AWS_IAM_ACCESS_KEY": ""
            }
        }
    ]
}