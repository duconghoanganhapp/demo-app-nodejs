<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>AdminLTE 3 | Dashboard</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="stylesheet" href="/css/fontawesome-free/all.min.css" />
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css"/>
    <link rel="stylesheet" href="/css/tempusdominus-bootstrap-4/tempusdominus-bootstrap-4.min.css"/>
    <link rel="stylesheet" href="/css/icheck-bootstrap/icheck-bootstrap.min.css"/>
    <link rel="stylesheet" href="/css/jqvmap/jqvmap.min.css" />
    <link rel="stylesheet" href="/css/dist/adminlte.min.css" />
    <link rel="stylesheet" href="/css/overlayScrollbars/OverlayScrollbars.min.css"/>
    <link rel="stylesheet" href="/css/daterangepicker/daterangepicker.css" />
    <link rel="stylesheet" href="/css/summernote/summernote-bs4.css" />
    <link rel="stylesheet" href="/css/select2/select2.min.css" />
    <link rel="stylesheet" href="/css/select2-bootstrap4-theme/select2-bootstrap4.min.css" />
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet"/>
    <link rel="stylesheet" href="/css/admin/admin.css" />
    <link rel="stylesheet" href="/sweetalert2/dist/sweetalert2.css">
    <!-- Specific CSS -->
    <%- style %>
  </head>
  <body class="hold-transition sidebar-mini layout-fixed text-sm">
    <div class="wrapper">
      <!-- NavBar -->
      <%- include('./navBar') %> 
      <!-- End - NavBar -->

      <!-- Body -->
      <div class="content-wrapper">

        <!-- Messages -->
        <%- include('./messages') %>
        <!-- End - Messages -->

        <%- body %>
      </div>

      <!-- SideBar -->
      <%- include('./sideBar') %>
      <!-- End - SideBar -->
      
      <!-- Footer -->
      <%- include('./footer') %>
      <!-- End - Footer -->

      <aside class="control-sidebar control-sidebar-dark">
      </aside>
    </div>
    <!-- Handle message -->
    <div id="handle-message" data-handle-message="<%= typeof messages != 'undefined' ? JSON.stringify(messages) : '' %>"></div>
    <!-- SCRIPT FILE -->
    <script src="/js/jquery/jquery.min.js"></script>
    <script src="/js/jquery-ui/jquery-ui.min.js"></script>
    <!-- VueJS-->
    <script src="https://cdn.jsdelivr.net/npm/vue@2.6.10/dist/vue.js"></script>
    <script>
      $.widget.bridge("uibutton", $.ui.button);
    </script>
    <script src="/js/bootstrap/bootstrap.bundle.min.js"></script>
    <script src="/js/chart/Chart.min.js"></script>
    <script src="/js/sparklines/sparkline.js"></script>
    <script src="/js/jquery-knob/jquery.knob.min.js"></script>
    <script src="/js/moment/moment.min.js"></script>
    <script src="/js/daterangepicker/daterangepicker.js"></script>
    <script src="/js/tempusdominus-bootstrap-4/tempusdominus-bootstrap-4.min.js"></script>
    <script src="/js/summernote/summernote-bs4.min.js"></script>
    <script src="/js/overlayScrollbars/jquery.overlayScrollbars.min.js"></script>
    <script src="/js/dist/adminlte.js"></script>
    <script src="/js/dist/demo.js"></script>
    <script src="/js/select2/select2.full.min.js"></script>
    <script type='module' src="/js/common.js"></script>
    <script type="module" src="/js/module.js"></script>
    <script src="/js/admin/admin.js"></script>
    <!-- Specific JS -->
    <%- script %>
  </body>
</html>
